@font-face {
    font-family: 'icomoon';
    src: url(data:application/font-woff;charset=utf-8;base64,) format('woff');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'icomoon';
    src: url(data:application/font-woff;charset=utf-8;base64,) format('woff');
    font-weight: normal;
    font-style: normal;
}
.navbar-box{
  width: 750rpx;
}
.navbar {
  width: 100%;
  height: inherit;
  position: fixed;
  z-index: 10000;
}
.navbar-nav {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 5px 20rpx 8px;
}
.navbar-title {
  max-width: 380rpx;
  margin: auto;
  font-size: 36rpx;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  line-height: 1.2;
}
.navbar-left{
  position: absolute;
  left: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 16px;
  border: 1rpx solid transparent;
}
.navbar-left2{
  border-color: transparent !important;
}
.navbar-left-light{
  border-color: rgba(0, 0, 0, 0.1);
  color: #000;
}
.navbar-left-dark{
  border-color: rgba(255, 255, 255, 0.3);
  color: #fff;
}
.navbar-btn{
  width: 80rpx;
  text-align: center;
  font-family:"icomoon" !important;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  font-size:14px;    
  line-height: 32px;
  color: inherit;
}
.navbar-btn:nth-child(2)::before{
  content: '';
  position: absolute;
  width: 1rpx;
  height: 16px;
  margin-left: -28rpx;
  margin-top: 8px;
  background-color: #bbb;
}
.navbar-redpackets{
  position: absolute;
  left: 100rpx;
  margin-top: 2px;
  font-size: 0;
}
.navbar-redpackets-img{
  display: block;
  width: 158rpx;
  height: 60rpx;
}

.add{
  position: fixed;
  right: 30rpx;
  width: 288rpx;
  height: 64rpx;
  border-radius: 10rpx;
  background: #2D6BF9;
  line-height: 64rpx;
  font-size: 24rpx;
  color:#fff;
  text-align: center;
  z-index: 10001;
}

.add::before{
  content: '';
  position: absolute;
  bottom: 60rpx;
  right: 106rpx;
  display: inline-block;
  width: 0;
  height: 0;
  border-width: 0 24rpx 20rpx 24rpx;
  border-style: solid;
  border-color: transparent transparent #2D6BF9 transparent;
}